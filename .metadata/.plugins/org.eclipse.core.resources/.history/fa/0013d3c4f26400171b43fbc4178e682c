import java.util.*;

public class test {

	public static void main(String[] args) {
		 Scanner in = new Scanner(System.in);
	        int t = in.nextInt();
	        for(int a0 = 0; a0 < t; a0++){
	            int n = in.nextInt();
	            int count =0;
	            while(n>=0){
	                int rem = n %10;
	                if(rem!=0 && n%rem==0){
	                    count++;
	                }
	               n = n/10;
	            }
	            System.out.println(count);
	        }
	}
}

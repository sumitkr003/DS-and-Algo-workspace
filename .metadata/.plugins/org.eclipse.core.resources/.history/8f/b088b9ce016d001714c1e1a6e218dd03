package july12;

public class StackClient {

	public static void main(String[] args) throws Exception{
	  DynamicStack st = new DynamicStack();
	  st.push(10);
	  st.display();

	  st.push(20);
	  st.display();

	  st.push(30);
	  st.display();

	  st.push(40);
	  st.display();

	  st.push(50);
	  st.display();
	  
	  st.push(60);
	  st.display();
	  
	  System.out.println(st.pop());
	  
	  System.out.println(st.pop());
	 
	  System.out.println(st.top());
	 
	 

	  
	}

}
